{
    "name": "Hermes-API",
    "version": "1.0.0",
    "description": "Hermes API for handling transporting bussines",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "watch": "nodemon server.js",
        "migrate": "knex migrate:latest --knexfile src/config/knex.js --cwd src/db",
        "rollback": "knex migrate:rollback --knexfile src/config/knex.js --cwd src/db",
        "seed": "knex seed:run --knexfile src/config/knex.js --cwd src/db"
    },
    "author": "sansajn",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "dotenv": "^6.1.0",
        "express": "^4.16.4",
        "faker": "^4.1.0",
        "knex": "^0.15.2",
        "mysql": "^2.16.0",
        "nodemon": "^1.18.7",
        "objection": "^1.4.0"
    }
}
